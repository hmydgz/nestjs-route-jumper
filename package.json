{
  "name": "nestjs-route-jumper",
  "displayName": "nestjs-route-jumper",
  "description": "test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "webview",
        "contents": "[测试按钮](command:git.clone)",
        "contents_doc": "内容: Markdown 的子集",
        "when": "false",
        "when_doc": "什么时候显示的表达式"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nestjs-route-jumper",
          "title": "Nestjs route jumper",
          "icon": "resources/logo.svg"
        }
      ]
    },
    "views": {
      "nestjs-route-jumper": [
        {
          "id": "webview",
          "type": "webview",
          "name": "Nestjs route jumper"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "npm run compile-src && npm run compile-web",
    "compile-src": "tsc -p ./src && node ./.vscode/package-src.js",
    "compile-web": "tsc -p ./web && node ./.vscode/package-web.js",
    "watch": "tsc -w -p ./src && node ./.vscode/package-src.js && tsc -w -p ./web && node ./.vscode/package-web.js",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "~20.10.5",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.35"
  }
}